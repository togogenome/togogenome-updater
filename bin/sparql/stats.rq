prefix sio:    <http://semanticscience.org/resource/>
prefix obo:    <http://purl.obolibrary.org/obo/>

SELECT ?gene_type (COUNT(DISTINCT ?gene_id) AS ?num_gene_id)
WHERE
{
 ?gene_id sio:SIO_000068  <http://identifiers.org/ncbigene> .
 ?gene rdfs:seeAlso ?gene_id .
 ?gene a ?gene_type .
 ?gene obo:RO_0002162  <http://identifiers.org/taxonomy/103690> .
} GROUP BY ?gene_type
