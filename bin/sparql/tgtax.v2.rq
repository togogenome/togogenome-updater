CONSTRUCT
{
  ?tax rdfs:subClassOf ?parent
}
FROM <http://togogenome.org/graph/stats>
FROM <http://togogenome.org/graph/taxonomy>
WHERE
{
  {
    SELECT DISTINCT ?tax
    WHERE
    {
      GRAPH <http://togogenome.org/graph/stats>
      {
        ?tax rdfs:seeAlso ?xref
        FILTER (STRSTARTS(STR(?tax), 'http://identifiers.org/taxonomy/')) .
      }
    }
  }
  GRAPH <http://togogenome.org/graph/taxonomy>
  {
    ?tax rdfs:subClassOf* ?parent .
  }
}
